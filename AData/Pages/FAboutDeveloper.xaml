<Page x:Class="ANode.AData.Pages.FAboutDeveloper"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ANode.AData.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="FAboutDeveloper">

    <Grid>
        <Grid HorizontalAlignment="Center" Height="430" VerticalAlignment="Center" Width="780">
            <StackPanel Orientation="Vertical" Width="Auto" Height="Auto" HorizontalAlignment="Center" VerticalAlignment="Center">
                <Grid x:Name="grid1" Height="80" Width="100">
                    <Image x:Name="cat_saying_welcome" Height="48" Width="48" Source="/Data/Resources/icons/developer.png" Stretch="Uniform"/>
                </Grid>
                <StackPanel>
                    <Grid Margin="0,0,0,0">
                        <TextBlock HorizontalAlignment="Stretch" TextWrapping="Wrap" Text="Разработчик: Керимов Али Муталимович" Height="32" Width="Auto"  VerticalAlignment="Stretch" FontSize="14" FontWeight="Bold" Foreground="#FF444B54"/>
                    </Grid>
                </StackPanel>
            </StackPanel>
        </Grid>
    </Grid>
</Page>
